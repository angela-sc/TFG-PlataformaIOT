@page "/analisis"

@using PortalWeb.ViewModel
@inherits AnalisisViewModel

<h3>Análisis</h3>

@if (proyectos != null)
{
    foreach(var proyecto in proyectos)
    {
        <div class="container mt-3">
            <div class="row">
                <div class="col-6">
                    <div class="btn-group dropright">
                        <button class="btn btn-primary dropdown-toggle" type="button" data-toggle="dropdown">@proyecto.Nombre</button>                        
                        <a class="dropdown-item"></a>
                    </div> 
                </div>
            </div>
        </div>
    }
}
else
{
    <p><em>Cargando ...</em></p>
}